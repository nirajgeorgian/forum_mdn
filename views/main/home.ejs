<% layout('boilerplate') -%>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<% if (message.length > 0) { %>
  <div class="alert alert-success alert-dismissible home-alert">
    <button class="close" data-dismiss="alert" aria-label="Close" type="button">
      <span aria-hidden="true">&times;</span>
    </button>
  <%= message %>
  </div>
<% } %>
</div>
</div>
